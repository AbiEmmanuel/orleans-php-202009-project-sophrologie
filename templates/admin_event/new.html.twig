{% extends 'layoutAdmin.html.twig' %}
{% form_theme form 'bootstrap_4_layout.html.twig' %}
{% block title %}Ajouter une actualité | {{ parent() }}{% endblock %}
    {% block stylesheets %}
      {{ encore_entry_link_tags('newEvent') }}
    {% endblock %}
{% block body %}

<main class="container ">
    <section class="sheettop">
        <div class="row">
          <div class="col-md-8 offset-md-2">
              <h1 class="title mb-5">Ajouter une actualité</h1>
          </div>
        </div>
        <div class="table-add-event">
            {% include 'admin_event/_form.html.twig' with {'label_button': 'Ajouter'} %}
        </div>

      <a class="btn btn-outline-primary mt-5" href="{{ path('admin_event_index') }}">Retour à la liste</a>

    </section>
  </main>

{% endblock %}



